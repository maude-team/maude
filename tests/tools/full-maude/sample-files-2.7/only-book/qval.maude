  fmod QVAL is 
    protecting QID-LIST .   
    extending VAL .
    sort QVal .   
    subsort QVal < Val .

    op ql : QidList -> QVal .
    eq compat(x:QVal,y:QVal) = true .
    var qidl : QidList .
    op qvalQidList : QVal -> QidList .
    eq qvalQidList(ql(qidl)) = qidl .
  endfm
