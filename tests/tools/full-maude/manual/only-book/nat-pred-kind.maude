  fmod NAT-PRED-KIND is
    sort Nat .
    op 0 : -> Nat [ctor] .
    op s_ : Nat -> Nat [ctor] .
    op p_ : Nat -> [Nat] .
    op _+_ : Nat Nat -> Nat [comm] .
    vars N M : Nat .
    eq N + 0 = N .
    eq (s N) + (s M) = s s (N + M) .
    eq p s N = N .
  endfm
    
